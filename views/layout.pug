doctype 
html 
    head 
        title=title
        link(rel='stylesheet' href='/style.css')
    body
        header
            nav
                ul
                    each item in config.menu
                        a(href=item[1])
                            div(class='menu_item')
                                li(id=item[0]) #{item[0]}
        h3(class="title")=title
        block content